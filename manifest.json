{
    "name": "Arebyte Wrong Extension",
    "version": "0.1",
    "description": "Arebyte - The Wrong Biennale. An exhibition as an extension.",
    "permissions": ["storage", "alarms", "idle"],
    "background": {
      "scripts": ["background.js"],
      "persistent": true
    },
    "browser_action": {
      "default_popup": "popup.html"
    },
    "icons": { 
      "16": "icons/rtc-16.png",
      "24": "icons/rtc-24.png",
      "32": "icons/rtc-32.png",
      "48": "icons/rtc-48.png",
      "128": "icons/rtc-128.png"
    },
    "manifest_version": 2
}
